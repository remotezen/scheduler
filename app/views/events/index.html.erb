<p id="notice"><%= notice %></p>
<h3>Shift at a glance</h3>
<%=   month_calendar events: @events do |date,events| %>
  <p class="month-day">
    <%= date.day %>
   </p>
  <% events.each do |event| %>
    <ul>
      <li class="name-in-list">
        <%= event.name %>
      </li>
      <li>
          start:
       
        <%=
        rTime(event.start_time)
      %>
      </li>
      <li class="last-table-list">
         end:
        <%= 
          rTime(event.end_time)
        %>
      </li>
    </ul>
    <% end %>
<% end %>
